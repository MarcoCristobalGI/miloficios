<p id="notice"><%= notice %></p>

<h1>Lista de Empleadores</h1>

<table class="table table-bordered">
  <thead class="thead-inverse">
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empleadors.each do |empleador| %>
      <tr>
        <td><%= empleador.nombres %></td>
        <td><%= empleador.apellidos %></td>
        <td><%= empleador.telefono %></td>
        <td><%= empleador.correo %></td>
        <td><%= link_to 'Show', empleador %></td>
        <td><%= link_to 'Edit', edit_empleador_path(empleador) %></td>
        <td><%= link_to 'Destroy', empleador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empleador', new_empleador_path %>
