<%= simple_form_for(@trabajador) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :apellidos %>
    <%= f.input :oficio %>
    <%= f.input :telefono %>
    <%= f.input :direccion %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
